<xs:schema version="1.0"
           targetNamespace="http://www.polytech-grenoble.fr/newMeteoVille"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.polytech-grenoble.fr/newMeteoVille" 
           elementFormDefault="qualified">

<xs:element name="meteoVille" type="tns:MeteoVille"/>
    <xs:complexType name="MeteoVille">
        <xs:sequence> 
            <xs:element name="ville" type="Ville"/>
            <xs:element name="date" type="xs:date"/>
            <xs:element name="temperatureMin" type="xs:double"/>
            <xs:element name="temperatureMax" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Ville">
        <xs:sequence>
            <xs:element name="nom" type="xs:string"/>
            <xs:element name="pays" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>   
</xs:schema>
