<xs:schema version="1.0"
           targetNamespace="http://www.polytech-grenoble.fr/MeteoVille"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.polytech-grenoble.fr/MeteoVille"
           elementFormDefault="qualified">
    
    <xs:element name="meteoVille" type="tns:MeteoVille"/>
    <xs:complexType name="MeteoVille">
        <xs:sequence>
            <xs:element name="ville" type="xs:string"/>
            <xs:element name="date" type="xs:date"/>
            <xs:element name="temperatureMin" type="xs:double"/>
            <xs:element name="temperatureMax" type="xs:double"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>
